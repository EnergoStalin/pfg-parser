    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug",
            "cwd": "${workspaceFolder}",
            "outputCapture": "std",
            "runtimeExecutable": "npm",
            "args": [
                "declaration",
                "16239655",
                "./16239655.json"
            ],
            "runtimeArgs": ["exec", "yarn", "run", "start:debug"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "merge",
            "cwd": "${workspaceFolder}/data",
            "program": "merge.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}
